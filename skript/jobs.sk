command /job:
	trigger:
		open chest with 2 rows named "&5          Liste des Jobs" to player
		wait 2 ticks
		format slot 1 of player with wooden pickaxe named "&3Mineur" with lore "" to close then run [make console execute "/joinmineur %player%"]
		format slot 3 of player with wooden axe named "&6Bûcheron" with lore "" to close then run [make console execute "/joinbucheron %player%"]
		format slot 5 of player with wooden sword named "&dChasseur" with lore "" to close then run [make console execute "/joinchasseur %player%"]
		format slot 7 of player with wooden hoe  named "&aFermier" with lore "&eprochainement .." to close
		
		format slot 9 of player with book  named "&bGerer mes métiers" with lore "" to run [make console execute "/gerermetier %player%"]	
		format slot 17 of player with red wool named "&4Fermer" with lore "" to close
		
command /gerermetier [<player>]:
	executable by: console
	trigger:
		open chest with 3 rows named "&5      Informations métiers" to arg 1
		wait 2 ticks
		format slot 3 of arg 1 with enchanted book named "%{job1.%arg 1%}%" with lore " ||&eNiveau &c= &b%{lvl_job1.%arg 1%}%||&eXp &c= &e%{xpjob1.%arg 1%}% &c/ &b%{xpmaxlvljob1.%arg 1%}%" to be unstealable
		format slot 5 of arg 1 with enchanted book named "%{job2.%arg 1%}%" with lore " ||&eNiveau &c= &b%{lvl_job2.%arg 1%}%||&eXp &c= &e%{xpjob2.%arg 1%}% &c/ &b%{xpmaxlvljob2.%arg 1%}%" to be unstealable		
		format slot 21 of arg 1 with red wool named "&4Abandonner %{job1.%arg 1%}%" with lore "&cCette action est déffinitive||&eVous perdrez vos lvl" to run [make console execute "/confirmationmetier1 %arg 1%"]
		format slot 23 of arg 1 with red wool named "&4Abandonner %{job2.%arg 1%}%" with lore "&cCette action est déffinitive||&eVous perdrez vos lvl" to run [make console execute "/confirmationmetier2 %arg 1%"]
			
command /confirmationmetier1 [<player>]:
	executable by:console
	trigger:
		open chest with 1 rows named "&4Quitter le métier %{job1.%arg 1%}% &4?" to arg 1
		wait 2 ticks
		format slot 3 of arg 1 with green wool named "&2Confirmer" with lore "" to close then run [make console execute "/leavejob1 %arg 1%"]
		format slot 5 of arg 1 with red wool named "&bAnnuler" with lore "" to close
	
command /confirmationmetier2 [<player>]:
