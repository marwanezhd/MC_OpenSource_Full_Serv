command /leavejob1 [<player>]:
	executable by: console
	trigger:
		send arg 1 title "&eMetier" with subtitle "&bTu viens d'abandonner le métier %{job1.%arg 1%}%" for 3 seconds
		set {lvl_job1.%arg 1%} to 1
		set {xp_job1.%arg 1%} to 0
		set {xpjob1.%arg 1%} to 0			
		set {xpmaxlvljob1.%arg 1%} to 0		
		set {job1.%arg 1%} to false		
		remove 1 from {nb_job.%arg 1%}

command /leavejob2 [<player>]:
