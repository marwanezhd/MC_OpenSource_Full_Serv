		format slot 5 of arg 1 with red wool named "&bAnnuler" with lore "" to close

command /joinmineur [<player>]:
	executable by:console
	trigger:
		if {nb_job.%arg 1%} >= 2:
			send "&6tu exerce déja &52 &bmétiers" to arg 1
			stop
		if {job1.%arg 1%} is "&3Mineur":
			send "&btu exerce déja le métier mineur" to arg 1
			stop
		if {job2.%arg 1%} is "&3Mineur":
			send "&btu exerce déja le métier mineur" to arg 1
			stop
		if {job1.%arg 1%} is not false:
			if {job2.%arg 1%} is not false:
				send "&bTu possède déja deux métiers" to arg 1
				stop
			else:
				set {job2.%arg 1%} to "&3Mineur"
				send "&bTu est à présent %{job2.%arg 1%}%" to arg 1
				add 1 to {nb_job.%arg 1%}
				set {xpmaxlvljob2.%player%} to {xpjob2.%player%} * {lvl_job2.%player%}
				stop
		else:
			set {job1.%arg 1%} to "&3Mineur"
			send "&bTu est a présent %{job1.%arg 1%}%" to arg 1
			add 1 to {nb_job.%arg 1%}
			set {xpmaxlvljob1.%player%} to {xpjob1.%player%} * {lvl_job1.%player%}
			stop
			
